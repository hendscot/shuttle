    "keywords": [
        "podcast",
        "minimalistic",
        "stream"
    ],
    "build": {
        "appId": "app.shuttle",
        "asar": false,
        "win": {
            "target": "squirrel",
            "icon": "build/rocket"
        }
    },
    "dependencies": {},
    "devDependencies": {
        "electron": "latest",
        "electron-builder": "^19.8.0"
    }
}