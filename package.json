{
    "name": "shuttle",
    "version": "0.0.8",
    "license": "MIT",
    "description": "Podcast Player",
    "author": "Scott Henderson",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "postinstall": "install-app-deps",
        "pack": "build --dir",
        "dist": "build"
    },
